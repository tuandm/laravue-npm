{
  "name": "laravue-components",
  "version": "0.1.0",
  "description": "Vue components for Laravue dashboard",
  "keywords": [
    "laravel",
    "laravue",
    "vue",
    "core"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name laravue ./src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "jsonlint": "^1.6.3",
    "script-loader": "^0.7.2",
    "tui-editor": "^1.3.3",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "./dist/laravue.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "author": {
    "name": "Tuan Duong",
    "email": "bacduong@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuandm/laravue-npm.git"
  }
}
